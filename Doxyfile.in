INPUT = @CMAKE_CURRENT_SOURCE_DIR@
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/docs/
PROJECT_NAME           = RemoteCL
PROJECT_BRIEF          = "Forward OpenCL API calls"
TAB_SIZE               = 4
BUILTIN_STL_SUPPORT    = YES
QUIET                  = YES
RECURSIVE              = YES
GENERATE_HTML          = YES
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_CLASSES  = YES
EXTRACT_ANON_NSPACES   = YES
JAVADOC_AUTOBRIEF      = YES
GENERATE_LATEX         = NO
WARN_IF_UNDOCUMENTED   = NO
FILE_PATTERNS          = *.cpp *.h
